{
  "name": "action-cache-s3",
  "version": "0.0.0",
  "private": true,
  "description": "Cache artifacts using S3",
  "main": "dist/restore/index.js",
  "scripts": {
    "build": "tsc && npm run build:restore && npm run build:save",
    "build:restore": "ncc build -o dist/restore src/restore.ts",
    "build:save": "ncc build -o dist/save src/save.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papix/action-cache-s3.git"
  },
  "keywords": [
    "actions",
    "node",
    "cache"
  ],
  "author": "papix",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.4",
    "@actions/io": "^1.0.2",
    "aws-sdk": "^2.814.0",
    "graceful-fs": "^4.2.4",
    "path": "^0.12.7",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/graceful-fs": "^4.1.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
